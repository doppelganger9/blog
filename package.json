{
  "name": "david-blog",
  "private": true,
  "description": "David Lacourt's personal blog, made with Sapper and Webpack, powered by Svelte and Markdown",
  "version": "2.0.0",
  "author": "David Lacourt <david.lacourt@gmail.com>",
  "bugs": {
    "url": "https://github.com/doppelganger9/blog/issues"
  },
  "homepage": "https://github.com/doppelganger9/blog#readme",
  "keywords": [
    "svelte",
    "sapper",
    "markdown",
    "blog",
    "javascript",
    "html",
    "css",
    "e2e",
    "jamstack"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doppelganger9/blog.git"
  },
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build",
    "export": "sapper export",
    "export:serve": "npx http-server __sapper__/export/",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:open-legacy": "cypress open --config baseUrl=http://localhost:8000 --env BLOG_JAMSTACK=gatsby",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "1.7.4",
    "polka": "0.5.2",
    "sirv": "0.4.6"
  },
  "devDependencies": {
    "cross-fetch": "3.0.5",
    "cypress": "4.8.0",
    "elizabot": "0.0.3",
    "marked": "1.1.0",
    "npm-run-all": "4.1.5",
    "rxjs": "6.6.0",
    "sapper": "0.27.16",
    "svelte": "3.23.2",
    "svelte-loader": "2.13.6",
    "webpack": "4.43.0"
  }
}
